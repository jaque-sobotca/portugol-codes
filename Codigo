programa
{
	
	funcao inicio()
	{
		cadeia nome
		cadeia cpf
		real salario

		escreva(" Digite o seu nome\n")
		leia(nome)
		escreva(" Qual seu CPF?\n")
		leia(cpf)
		escreva(" Qual é o seu salário?\n")
		leia(salario)
		escreva(nome + ", seu cpf é " + cpf + " seu salário é " + salario)
		
	}
}
